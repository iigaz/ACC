<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>ACC.ACCollections</PackageId>
        <Version>0.0.1</Version>
        <Title>ACC</Title>
        <Authors>iigaz</Authors>
        <Description>A's Custom Collections! Набор коллекций, основанных на связном списке.</Description>
        <RepositoryUrl>https://github.com/iigaz/ACC</RepositoryUrl>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <DocumentationFile>bin\Debug\net6.0\ACCollections.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
        <DocumentationFile>bin\Release\net6.0\ACCollections.xml</DocumentationFile>
    </PropertyGroup>

</Project>
